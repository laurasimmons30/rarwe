<div class="containers">
  <div class="pager-headers">
    <h1>Rock & Roll<small> with Ember.js</small></h1>
  </div>
  <div class="row">
    <div class="col-md-6">
      <ul>
        {{#each songs.content as |song|}}
          <li class="list-group-item">{{song.title}} by <em>{{song.band}}</em> 
          <span class="rating pull-right">({{song.rating}})</span></li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>